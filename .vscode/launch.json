{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Iphone Simulator",
            "request": "launch",
            "type": "dart",
            "deviceId": "Iphone SE",
            "cwd": "${workspaceFolder}",
            "program": "lib/main.dart"
        },
        {
            "name": "forum_firebase_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "forum_firebase_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "forum_firebase_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}